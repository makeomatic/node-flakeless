{
  "name": "ms-flakeless",
  "version": "4.0.1",
  "description": "Generates unique IDs from timestamps",
  "main": "lib/flakeless.js",
  "keywords": [
    "snowflake",
    "flakeless",
    "unique",
    "id"
  ],
  "scripts": {
    "pretest": "yarn lint",
    "lint": "eslint .",
    "test": "mocha",
    "install": "node-gyp rebuild",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preparecommitmsg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/makeomatic/node-flakeless"
  },
  "author": "Anthony Salani (https://github.com/asalani93)",
  "license": "MIT",
  "devDependencies": {
    "@makeomatic/deploy": "^8.4.5",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-makeomatic": "^3.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "node-gyp": "^5.0.3"
  },
  "dependencies": {
    "nan": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
